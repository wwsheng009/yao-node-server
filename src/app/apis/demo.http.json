{
    "name": "调试",
    "version": "1.0.0",
    "description": "调试本地yao js脚本",
    "group": "local/demo",
    "guard": "",
    "paths": [
        {
            "guard": "-",
            "path": "/query",
            "method": "get",
            "process": "utils.str.Concat",
            "in": [
                "$query.name",
                "$query.age"
            ],
            "out": {
                "status": 200,
                "type": "application/json"
            }
        },
        {
            "guard": "-",
            "path": "/form",
            "method": "POST",
            "process": "utils.str.Concat",
            "in": [
                "$form.name",
                "$form.password"
            ],
            "out": {
                "status": 200,
                "type": "application/json"
            }
        },
        {
            "guard": "-",
            "path": "/file",
            "method": "POST",
            "process": "scripts.fs.Print",
            "in": [
                "$file.foo"
            ],
            "out": {
                "status": 200,
                "type": "application/json"
            }
        },
        {
            "guard": "-",
            "path": "/print/:id",
            "method": "get",
            "process": "scripts.fs.Print",
            "in": [
                "$param.id"
            ],
            "out": {
                "status": 200,
                "type": "application/json"
            }
        },
        {
            "guard": "-",
            "path": "/body/:id",
            "method": "get",
            "process": "scripts.script3.body",
            "in": [
                "$param.id"
            ],
            "out": {
                "status": 200,
                "type": "application/json",
                "body": "{{body.message}}"
            }
        }
    ]
}